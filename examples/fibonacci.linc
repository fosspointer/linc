// The fibonacci number sequence demonstrated using linc!

fn display_fibonacci(count: u8) {
    first: mut u32 = 0u32;
    second: mut u32 = 1u32;

    number_of_fibonacci: u8 = for index in 0u8..count {
        first = (first + second);
        second = (first - second);
        
        putln("#" + @(index + 1u8) + ": " + @first);
        index
    };

    putln("The fibonacci number count displayed was " + @number_of_fibonacci + '.');
}

fn main() {
    display_fibonacci(20u8);
}
