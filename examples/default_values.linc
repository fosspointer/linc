// demonstration of default values in linc!

#include `std.linc`

fn call(greeting: string = "Hello", first_name: string = "John", last_name: string = "Doe") {
    println(greeting + " \"" + first_name + ' ' + last_name + '"');
}

fn main() {
    call();
    call("Hey");
    call("Hey", "Jane");
    call("Hey", "Jane", "Smith");
}