# Showcase of while/finally/else loop

{
    u8 i mut = 0u8

    while i <= 10u8 {
        puts("Square #" + @i + ": " + @(i * i) + 10c)
        ++i
    }
    finally {
        puts("The loop came to an end before index " + @i + 10c)
    }
    else {
        puts("The loop never even ran" + 10c)
    }
}